Algoritmo operaciones_2_numeros
	Escribir "Ingresar dos numeros";
	Leer a, b;
	suma = a + b;
	resta = a - b;
	multiplicar = a * b;
	division_decimal = a / b;
	division_entera = trunc( a / b);
	modulo = a MOD b;
	Escribir  "Suma ", suma;
	Escribir "Multiplicacion ", multiplicar;
	Escribir "Division decimal ", division_decimal;
	Escribir "Division entera ", division_entera;
	Escribir "Modulo ", modulo;
FinAlgoritmo